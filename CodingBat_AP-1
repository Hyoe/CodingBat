//scoresSpecial
public int scoresSpecial(int[] a, int[] b) {
  int sum = 0;
  sum = largestSpecialScore(a) + largestSpecialScore(b);
  return sum;
}

public int largestSpecialScore(int[] newArray) {
  int largest = 0;

  for (int i = 0; i < newArray.length; i++) {
    if (newArray[i] % 10 == 0 && newArray[i] > largest) {
      largest = newArray[i];
    }
  }
  return largest;
}

