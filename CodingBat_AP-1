//userCompare
public int userCompare(String aName, int aId, String bName, int bId) {
  if (aName.compareTo(bName) > 0) {
    return 1;
  }
  else if (aName.equals(bName)) {
    if (aId > bId) {
      return 1;
    }
    else if (aId < bId) {
      return -1;
    }
    else {
      return 0;
    }
  }
  return -1;
}

