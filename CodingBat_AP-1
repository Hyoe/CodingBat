//hasOne
public boolean hasOne(int n) {
  while (n > 0) {
    if (n % 10 == 1) {
      return true;
    }
    else {
      n = n / 10;
    }
  }
  return false;
}

—————

//dividesSelf
public boolean dividesSelf(int n) {
  int originalN = n;
  while (n > 0) {
    if (n % 10 == 0) {
      return false;
    }
    else if (originalN % (n % 10) == 0) {
      n = n / 10;
    }
    else {
      return false;
    }
  }
  return true;
}

